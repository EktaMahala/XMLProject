@page
@model GroceryStores.Pages.Surveys.IndexPageModel
@{
    ViewData["Title"] = "IndexPage";
}

<h1>IndexPage</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.surveyList[0].firstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.surveyList[0].lastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.surveyList[0].mobile)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.surveyList[0].email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.surveyList[0].zip)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.surveyList[0].stores)
            </th>
           
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.surveyList)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.firstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.lastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.mobile)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.zip)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.stores)
                </td>
                
            </tr>
        }
    </tbody>
</table>
